<div class="row">
  <div style="margin-left:120px;">
    <% if user_signed_in? %>
      <%= link_to "マイページ", user_path(current_user) %>
      <%= link_to "ログアウト", destroy_user_session_path, method: :delete %>
    <% else %>
      <%= link_to "ログイン", new_user_session_path %>
      <%= link_to "新規登録", new_user_registration_path %>
    <% end %>
  </div>
  <div class="row color-change">
    <div class="change-red">赤</div>
    <div class="change-blue">青</div>
    <div class="change-yellow">黄</div>
    <div class="change-green">緑</div>
    <div class="change-white">白</div>
  </div>
</div>